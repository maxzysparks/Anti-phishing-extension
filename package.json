{
  "name": "anti-phishing-extension",
  "version": "1.0.0",
  "description": "Browser extension to detect phishing and malicious links in Gmail and Outlook",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "lint": "eslint src tests",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxzysparks/Anti-phishing-plugin.git"
  },
  "keywords": [
    "phishing",
    "security",
    "email",
    "gmail",
    "outlook",
    "chrome-extension"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxzysparks/Anti-phishing-plugin/issues"
  },
  "homepage": "https://github.com/maxzysparks/Anti-phishing-plugin#readme",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@testing-library/jest-dom": "^6.1.0",
    "babel-jest": "^29.7.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "jest": {
    "testEnvironment": "jsdom"
  }
}
